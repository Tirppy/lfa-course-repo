digraph {
	q1 [label=q1 shape=ellipse]
	q0 [label="start
q0" fillcolor=lightblue shape=ellipse style=filled]
	q2 [label=q2 shape=ellipse]
	q3 [label="final
q3" shape=doublecircle]
	q0 -> q1 [label=a]
	q0 -> q2 [label=a]
	q1 -> q1 [label=b]
	q1 -> q2 [label=a]
	q2 -> q1 [label=a]
	q2 -> q3 [label=b]
}
